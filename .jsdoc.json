{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "opts": {
        "destination": "./docs/docs",
        "encoding": "utf8",
        "recurse": true,
        "tutorials": "./docs/tutorials",
        "template": "node_modules/better-docs"
    },
    "source": {
        "include": [
            "./src/",
            "README.md"
        ],
        "includePattern": ".(j|t)s$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-babel"
    ],
    "templates": {
        "referenceTitle": "GameKit",
        "disableSort": false,
        "collapse": true,
        "better-docs": {
            "name": "GameKit Documentation",
            "navigation": [
                {
                    "label": "Coding",
                    "href": "https://warmrabbit.coding.net/p/GameKit/git/tree/master"
                }
            ]
        }
    },
    "babel": {
        "extensions": [
            "ts",
            "tsx"
        ],
        "ignore": [
            "**/*.(test|spec).ts"
        ],
        "babelrc": false,
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": true
                    }
                }
            ],
            "@babel/preset-typescript"
        ],
        "plugins": [
            "@babel/proposal-class-properties",
            "@babel/proposal-object-rest-spread"
        ]
    }
}